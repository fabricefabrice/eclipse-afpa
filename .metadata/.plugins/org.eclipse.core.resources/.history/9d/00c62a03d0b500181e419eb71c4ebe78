
public class TypeVariablesJava {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Declarationn et Affectation
		
		//Declaration
		// TYPE  nomVariable = valeur ;
		int  monInt = 12 ;
		double monDouble = 15.5 ;
		char monChar = 'a' ;
		boolean monBoolean = false  ;
		String maChaine = "chaine" ;
		float monFloat = 13.5f ;
		
		//Affectation
		//nomVariable = valeur ;
		maChaine = "nouvelle chaine" ;
		monChar = 'Z' ;
		monBoolean = true ;
		monDouble = 120.12  ;
		monInt = 1500 ;
		monFloat = 155.26f ;
		System.out.println("Affectation :") ;
		System.out.println("monInt =" + monInt;) ;
		
		
		
		
		
		

	}

}
