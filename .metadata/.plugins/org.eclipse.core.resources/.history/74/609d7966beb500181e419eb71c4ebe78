
public class Condition {

	public static void main(String[] args) {
		
		
		int prix = 10 ;
		
		if (prix == 10) {
			System.out.println("le prix = 10") ;
	}
		
		double decimal = 15.5 ;
		
		if (decimal = 15.5)  {
			System.out.println("la variable decimal = 15.5") ;
		}
			
		
			

	}
		
		}

	}

