
public class SwitchCondition {
	
	int entier = 15 ;
	
	switch (entier) {
	case 10 :
	 System.out.println("La variable entier = 10") ;
	 break ;
	case 15 :
	  System.out.println("La variable entier = 15") ;
	  break ;
	case 13 :
	 System.out.println("La variable entier = 13") ;
	 break ;
	 default :
	 System.out.println("Afficher par defaut") ;
	 break ;
	}

}
